{
  "extends": [
    "@opengovsg/eslint-config-opengovsg",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "mocha",
    "@typescript-eslint"
  ],
  "overrides": [{
    "files": "*.test.js",
    "rules": {
      "no-unused-expressions": "off"
    }
  }],
  "env": {
    "mocha": true,
    "node": true
  }
}
